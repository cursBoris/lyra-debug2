{
  "name": "frontend",
  "version": "1.0.0",
  "description": "Lyra Grid example project",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "inspect:dev": "vue-cli-service inspect",
    "inspect:prod": "vue-cli-service inspect  --mode production",
    "cache:clean": "npm cache clean --force",
    "lint:fix": "eslint --fix  --no-ignore --config node_modules/lyra-grid/.eslintrc.js node_modules/lyra-grid/src/grid.js node_modules/lyra-grid/src/grid2.js node_modules/lyra-grid/src/LyraGrid.vue node_modules/lyra-grid/src/store.js node_modules/lyra-grid/src/util.js"
  },
  "dependencies": {
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "bootstrap-vue": "^2.0.2",
    "lyra-grid": "^1.2.0"
  },
  "devDependencies": {
    "@vue/cli-service": "^4.0.0-rc.6",
    "node-sass": "^4.12.0",
    "sass-loader": "^8.0.0",
    "vue-template-compiler": "^2.6.10",
    "dojo-webpack-plugin": "^2.8.9",
    "copy-webpack-plugin": "^5.0.4",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-vue": "^5.2.3"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
